{
  "manifest_version": 3,
  "key": "",
  "name": "First Glance - Focus Tab",
  "description": "Turn your new tab into a focus dashboard. View high-priority tasks at a glance. Stay productive, avoid distractions.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/index.html"
  },
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "permissions": ["storage", "identity"],
  "host_permissions": ["https://api.dida365.com/*", "https://dida365.com/*"]
}
